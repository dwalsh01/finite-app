{
  "name": "dota-tailwind",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "@types/recharts": "^1.8.4",
    "@types/yup": "^0.26.27",
    "apollo": "^2.21.2",
    "apollo-boost": "^0.4.7",
    "apollo-cache-inmemory": "^1.6.5",
    "autoprefixer": "^9.7.3",
    "formik": "^2.0.8",
    "graphql": "^14.5.8",
    "postcss-cli": "^6.1.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "recharts": "^2.0.0-beta.1",
    "typescript": "~3.7.2",
    "yup": "^0.28.0"
  },
  "scripts": {
    "start": "yarn run tailwind:css && react-app-rewired start",
    "tailwind:css": "tailwind build src/styles/index.css -o src/styles/tailwind.css",
    "build": "yarn run tailwind:css && react-app-rewired  build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "server": "cd server && yarn start",
    "schema:download": "apollo schema:download schema.json --endpoint=http://localhost:4000/graphql",
    "codegen:generate": "apollo codegen:generate --localSchemaFile=schema.json --queries=./src/queries/*.ts --outputFlat --target=typescript src/types",
    "gen:types": "yarn run schema:download && yarn run codegen:generate"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.7.4",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-plugin-macros": "^2.8.0",
    "customize-cra": "^0.9.1",
    "emotion": "^10.0.27",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "prettier": "^1.19.1",
    "react-app-rewired": "^2.1.5",
    "tailwind.macro": "^1.0.0-alpha.10",
    "tailwindcss": "^1.1.4"
  }
}
